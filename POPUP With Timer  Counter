let logoutTime = {
     showWarningModal : function(){
	  let time = this.setTimeforWarningModal("10000")
	  console.log(time);
      setTimeout(function(){
      $("#warningModal").modal("show");
	  logoutTime.TimerOnModal().timer();
      },time); 
	  
	 },
	 hideWarningModal : function(){
      $("#warningModal").modal("hide");
	 },
	 setTimeforWarningModal : function($time){
	 if($time && !isNaN($time)){
	  return $time
	 } else{throw "time must be numeric"};
	
	 },
	 clearValue : null,
	 settimeFortimer :  60,
	 TimerOnModal : function(){
	 var TimerOnModalDetial={};
	 TimerOnModalDetial.timer = function(){
	 document.getElementById('mycounter').innerHTML = logoutTime.settimeFortimer;
     logoutTime.settimeFortimer--;
     if (logoutTime.settimeFortimer < 0) {
     alert('You are logging out!');
     }
    else {
    logoutTime.clearValue = setTimeout(function(){
	TimerOnModalDetial.timer();
	}, 1000);
	
     }
	 };
	 TimerOnModalDetial.timeclear = function(){
	 console.log(logoutTime.clearValue);
	 clearTimeout(logoutTime.clearValue);
         logoutTime.clearValue = null;
		 logoutTime.hideWarningModal();
		 //-----extent logoutTime here---------//
	 };
	 return TimerOnModalDetial;
	 },
	 resetLogOutTime : function(){
	  console.log("Ok clicked")
	 
	 };
	 
	 event : function(){
	 this.showWarningModal();
	
	 }
	 
	 
}
logoutTime.event();

//-----------------HTML POP UP--------------------------------//
<div id="warningModal" class="modal fade" role="dialog">
  <div class="modal-dialog modal-md">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Warning</h4>
      </div>
      <div class="modal-body">
        <p>You are about to logout.</p>
      </div>
      <div class="modal-footer">
	    <div class="col-sm-6 text-left">
         Time remaining : <span id="mycounter"></span>
		 </div>
		 
	   <div class="col-sm-6">
        <button type="button" class="btn btn-default"  onclick="logoutTime.TimerOnModal().timeclear();" id="cancelModal">Cancel</button>
		 <button type="button" class="btn btn-success" onclick="logoutTime.resetLogOutTime();" >OK</button>
		 </div>
		 <div class="clearfix"></div>
      </div>
    </div>

  </div>
</div>
